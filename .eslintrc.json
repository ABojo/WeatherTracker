{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "linebreak-style": 0,
    "no-unused-vars": "off",
    "func-names": "off",
    "no-console": "off",
    "comma-dangle": "off",
    "wrap-iife": "off",
    "no-undef": "off"
  }
}
